{
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "Basic react boilerplate",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist/",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "build": "yarn clean && yarn webpack -p --env.env=prod",
    "build:analyze": "yarn build --env.addon=bundleanalyze",
    "dev": "yarn webpack-dev-server --env.env=dev",
    "dev:analyze": "yarn dev --env.addon=bundleanalyze"
  },
  "repository": {
    "type": "git",
    "url": "git+https://GazzaHazza@bitbucket.org/GazzaHazza/boilerplate.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/GazzaHazza/boilerplate#readme",
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.3.13",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.8.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.5.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.9.4",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "html-webpack-plugin": "^2.30.1",
    "prettier": "^1.8.2",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "rimraf": "^2.6.2",
    "webpack": "^3.8.1"
  }
}